package exercicio1;

public class Cidade extends Endereco {
	private String nomeCidade;
	private Estado estado;

	public Cidade(String nome, Telefone[] telefones, String rua, String bairro, String cep, Cidade cidade,
			String nomeCidade, Estado estado) {
		super(nome, telefones, rua, bairro, cep, cidade);
		this.nomeCidade = nomeCidade;
		this.estado = estado;
	}

	public String getNomeCidade() {
		return nomeCidade;
	}

	public Estado getEstado() {
		return estado;
	}

}
