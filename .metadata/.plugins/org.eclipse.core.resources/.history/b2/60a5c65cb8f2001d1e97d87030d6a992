package exercicio1;

public class Endereco extends Contato {
	private String rua;
	private String bairro;
	private String cep;
	private Cidade cidade;

	public Endereco(String nome, Telefone[] telefones, Endereco endereco, String rua, String bairro, String cep,
			Cidade cidade) {
		super(nome, telefones, endereco);
		this.rua = rua;
		this.bairro = bairro;
		this.cep = cep;
		this.cidade = cidade;
	}

	public String getRua() {
		return rua;
	}

	public String getBairro() {
		return bairro;
	}

	public String getCep() {
		return cep;
	}

	public Cidade getCidade() {
		return cidade;
	}

}
